# Copy this file to .env and fill in your actual values

# LLM Configuration (Choose: venice or gemini)
LLM_PROVIDER=venice

# Venice AI Configuration (Primary - Llama 3.3 70B)
VENICE_API_KEY=your-venice-api-key
VENICE_API_URL=https://api.venice.ai/api/v1
VENICE_MODEL=llama-3.3-70b

# Google Gemini Configuration (Alternative)
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-1.5-pro

# Database (Optional for MVP - using local JSON file)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# Google Calendar API (Optional - using mock data for MVP)
GOOGLE_CALENDAR_API_KEY=your-api-key
GOOGLE_CALENDAR_CLIENT_ID=your-client-id
GOOGLE_CALENDAR_CLIENT_SECRET=your-client-secret

# Twilio (Optional - console logging for MVP)
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Blockchain (Optional - using mock data for MVP)
BASE_RPC_URL=https://sepolia.base.org
BASE_MAINNET_RPC_URL=https://mainnet.base.org
CONTRACT_ADDRESS=0x...
WALLET_PRIVATE_KEY=your-private-key

# OPIC Integration (Optional - file logging for MVP)
OPIC_API_KEY=your-opic-key
OPIC_PROJECT_ID=your-project-id

# Application
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000
