# Copy this file to .env and fill in your actual values

# OpenAI Configuration (REQUIRED for agent to work)
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4o

# Database (Optional for MVP - using local JSON file)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# Google Calendar API (Optional - using mock data for MVP)
GOOGLE_CALENDAR_API_KEY=your-api-key
GOOGLE_CALENDAR_CLIENT_ID=your-client-id
GOOGLE_CALENDAR_CLIENT_SECRET=your-client-secret

# Twilio (Optional - console logging for MVP)
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Blockchain (Optional - using mock data for MVP)
BASE_RPC_URL=https://sepolia.base.org
BASE_MAINNET_RPC_URL=https://mainnet.base.org
CONTRACT_ADDRESS=0x...
WALLET_PRIVATE_KEY=your-private-key

# OPIC Integration (Optional - file logging for MVP)
OPIC_API_KEY=your-opic-key
OPIC_PROJECT_ID=your-project-id

# Application
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000
